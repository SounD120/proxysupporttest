{
	"name": "proxysupporttest",
	"displayName": "ProxySupportTest",
	"description": "proxysupporttest",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:proxysupporttest.startServer",
		"onCommand:proxysupporttest.getRequest"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "proxysupporttest.startServer",
				"title": "Start test HTTP server"
			},
			{
				"command": "proxysupporttest.getRequest",
				"title": "Sends HTTP request to server"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.27"
	}
}
